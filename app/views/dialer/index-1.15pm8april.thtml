function upload_photo(i) {
	if($('input[id="sortpicture'+i+'"]').val().length == 0) {
		return;
	}
	var formData = new FormData();
	formData.append('section', 'general');
	formData.append('action', 'previewImg');
	// Main magic with files here
	formData.append('image', $('input[id="sortpicture'+i+'"]')[0].files[0]);
	$('#uploading_' + i).show();

	$.ajax({
		url: '/acesys/index.php/orders/uploadPhoto//'+i,
		dataType: 'text',
		cache: false,
		contentType: false,
		processData: false,
		data: formData,
		type: 'post',
		success: function(data) {
			$('#photo_' + i).attr('src', data);
			$('#big_photo_' + i).attr('src', data);
			$('#uploading_' + i).hide();
		}
	});
}
