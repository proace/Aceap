		url: '<?=BASE_URL?>/orders/checkMailSend/?ordid=<?=$this->data['Order']['id'] ?>',
		dataType: 'text',
		cache: false,
		contentType: false,
		processData: false,
		data: formData,
		type: 'post',
		success: function(data) {
		var res = data.split("@@");
			if(res[0]==1){
				$("#msgbx").html("You have already sent an email and text to this client on ("+res[1]+") would like to send another email/text  with all changes?");
				showme();

