				url: "/acesys/vicidial/call.php",
				cache: false,
				success: function (d) {
				  $('#'+dial_id).hide();
				  $('#'+hangup_id).show();
				  
					console.log(d);
				},
				error: function (d) {
					console.log(d);
				}
			});
	    }
